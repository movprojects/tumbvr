{
  "name": "TumbVR",
  "version": "1.0.0",
  "description": "A museum in VR with aFrame and Tumblr",
  "main": "server.js",
  "devDependencies": {
    "browserify": "*",
    "better-npm-run": "~0.0.1",
    "nodemon": "^1.9.2",
    "uglify-js": "^2.6.1",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "watch": "watchify client/js/app/main.js -o public/app.js -v",
    "build": "browserify client/js/app/main.js | uglifyjs -cm > public/prod.js",
    "dev": "better-npm-run dev",
    "prod": "better-npm-run prod",
    "start": "node server.js"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon server.js",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "prod": {
      "command": "nodemon server.js",
      "env": {
        "NODE_ENV": "prod"
      }
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "tumblr.js": "^1.0.0",
    "cors-anywhere" : "*"
  }
}
